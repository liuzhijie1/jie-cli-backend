{
  "name": "egg-ts-boilerplate",
  "version": "1.1.0",
  "description": "Boilerplate for Egg.js app using TypeScript.",
  "egg": {
    "typescript": true,
    "require": [
      "egg-ts-helper/register"
    ]
  },
  "dependencies": {
    "@vue/server-renderer": "^3.4.27",
    "axios": "^0.21.4",
    "busboy": "^0.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.11.0",
    "dotenv": "^10.0.0",
    "egg": "^2.14.2",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "^2.2.4",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "filemanager-webpack-plugin": "^6.1.7",
    "html-webpack-plugin": "^5.6.0",
    "jie-cli-components": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lego-components": "^0.1.7",
    "mini-css-extract-plugin": "^2.9.0",
    "mongoose": "^5.10.9",
    "mongoose-sequence": "^5.3.1",
    "nanoid": "^3.3.7",
    "sharp": "^0.29.3",
    "stream-wormhole": "^2.0.1",
    "vue": "^3.4.27",
    "webpack": "^5.91.0",
    "webpack-cli": "^4.10.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/busboy": "^0.3.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^5.2.5",
    "@types/node": "^16.11.7",
    "@types/sharp": "^0.29.5",
    "@types/supertest": "^2.0.7",
    "egg-bin": "^4.10.0",
    "egg-mock": "^3.21.0",
    "egg-ts-helper": "^1.20.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^8.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-ts-boilerplate",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "build:template": "npx webpack --config webpack/webpack.config.js",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "upload": "node webpack/uploadToOSS.js"
  },
  "ci": {
    "version": "8, 10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brickyang/egg-ts-boilerplate.git"
  },
  "author": "brick <brick.c.yang@gmail.com>",
  "license": "MIT"
}
